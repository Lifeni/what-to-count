{
  "name": "what-to-count",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development elecrun --vite --preload preload.ts",
    "preview": "yarn build && cross-env NODE_ENV=production electron ./app/",
    "build": "elecrun build --preload preload.ts && vite build",
    "clean": "elecrun clean",
    "pack": "yarn run build && yarn run rebuild && electron-builder -c ./electron-builder.yml --win --linux",
    "pack:all": "yarn run build && yarn run rebuild && electron-builder -c ./electron-builder.yml --win --linux --mac",
    "pack:win": "yarn run build && yarn run rebuild && electron-builder -c ./electron-builder.yml --win",
    "pack:mac": "yarn run build && yarn run rebuild && electron-builder -c ./electron-builder.yml --mac",
    "pack:linux": "yarn run build && yarn run rebuild && electron-builder -c ./electron-builder.yml --linux",
    "rebuild": "electron-rebuild -m ./app",
    "postinstall": "electron-builder install-app-deps",
    "prettier": "prettier -w ."
  },
  "devDependencies": {
    "@types/node": "^16.4.10",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.3.1",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.6",
    "electron": "^13.1.7",
    "electron-builder": "^22.11.7",
    "electron-log": "^4.4.1",
    "electron-rebuild": "^2.3.5",
    "electron-run": "^2.0.1",
    "electron-store": "^8.0.0",
    "esbuild": "^0.12.17",
    "localforage": "^1.9.0",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "tailwindcss": "^2.2.7",
    "ts-node": "^10.1.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "vite": "^2.4.4"
  }
}